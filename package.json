        "lezer-python": "^0.13.1",
        "ts-loader": "^8.0.13",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3",
        "wabt": "^1.0.20",
        "webpack": "^5.11.1",
        "webpack-cli": "^4.3.1"
    },
    "devDependencies": {
        "@types/chai": "^4.2.14",
